<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.dvnchina.advertDelivery.model">
	<class name="PreciseMatch" table="T_PRECISE_MATCH">

		<id name="id" type="java.lang.Integer" column="id">
			<generator class="native">
				<!--<param name="sequence">T_PRECISE_SEQ</param>-->
			</generator>
		</id>
		
		<property name="matchName" column="MATCH_NAME" type="java.lang.String" />

		<property name="type" column="TYPE" type="java.lang.Integer" />

		<property name="productCode" column="PRODUCT_CODE" type="java.lang.String" />
		
		<property name="productName" column="PRODUCT_NAME" type="java.lang.String" />
		
		<property name="btvChannelId" column="BTV_CHANNEL_ID" type="java.lang.String" />
		
		<property name="btvChannelName" column="BTV_CHANNEL_NAME" type="java.lang.String" />

		<property name="assetSortId" column="ASSET_SORT_ID" type="java.lang.String" />

		<property name="key" column="KEY" type="java.lang.String" />

		<property name="userArea" column="USER_AREA" type="java.lang.String" />
		
		<property name="userAreaName" column="USER_AREA_NAME" type="java.lang.String" />

		<property name="userIndustrys" column="USERINDUSTRYS" type="java.lang.String" />
		
		<property name="userIndustrysName" column="USERINDUSTRYS_NAME" type="java.lang.String" />
		
		<property name="userLevelsName" column="USERLEVELS_NAME" type="java.lang.String" />
		
		<property name="tvnNumber" column="TVN_NUMBER" type="java.lang.String" />

		<property name="priority" column="PRIORITY" type="java.lang.Integer" />
		
		<property name="ployId" column="PLOY_ID" type="java.lang.Integer" />

		<property name="categoryId" column="CATEGORY_ID" type="java.lang.String" />
		
		<property name="groupId" column="CHANNEL_GROUP_ID" type="java.lang.String" />
	</class>
</hibernate-mapping>