<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dvnchina.advertDelivery.position.bean.VideoSpecification" table="T_VIDEO_SPECIFICATION">
        <id name="id" type="java.lang.Integer">
            <column name="ID" length="10" />
        </id>
        <property name="movieDesc" type="java.lang.String">
            <column name="MOVIE_DESC" />
        </property>
        <property name="resolution" type="java.lang.String">
            <column name="RESOLUTION" />
        </property>
        <property name="duration" type="java.lang.Integer">
            <column name="DURATION" />
        </property>
        <property name="type" type="java.lang.String">
            <column name="TYPE" />
        </property>
        <property name="fileSize" type="java.lang.String">
            <column name="FILE_SIZE" />
        </property>
    </class>
</hibernate-mapping>