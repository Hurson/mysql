<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dvnchina.advertDelivery.sysconfig.bean.AreaOCG" table="T_OCGINFO"  >
        <comment>各区域ocg版本号</comment>
        <id name="id" type="java.lang.Integer">
            <column name="ID" precision="38" scale="0" />
             <generator class="assigned">
			</generator>
        </id>
        <property name="ip" type="java.lang.String">
            <column name="IP" length="32">
                <comment>ip</comment>
            </column>
        </property>
        <property name="port" type="java.lang.String">
            <column name="PORT" length="32">
                <comment>端口</comment>
            </column>
        </property>
        <property name="user" type="java.lang.String">
            <column name="USER" length="32">
                <comment>用户名</comment>
            </column>
        </property>
        <property name="pwd" type="java.lang.String">
            <column name="PWD" length="32">
                <comment>密码</comment>
            </column>
        </property>
        <property name="areaCode" type="java.lang.String">
            <column name="AREACODE" length="32">
                <comment>区域码</comment>
            </column>
        </property>
        <property name="version" type="java.lang.String">
            <column name="VERSION" length="32">
                <comment>版本号</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>






